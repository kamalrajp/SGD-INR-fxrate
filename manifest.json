{
    "manifest_version": 2,
    "name": "SGD/INR Fx Rate",
    "description": "SGD/INR exchange rate",
    "version": "1.0.0",
    "icons": {
        "128": "img/icon_128.png"
    },
    "permissions": [
        "storage",
        "alarms",
        "tabs",
        "*://*/*"
    ],
    "browser_action": {
        "default_icon": "img/icon.png"
    },
    "background": {
        "scripts": [
           "jquery-3.3.1.min.js",
           "storage.js",
           "background.js"
        ]
    }
}